CREATE TABLE COEMP(EmpId INTEGER PRIMARY KEY, Name TEXT);
CREATE TABLE PTSLS(SlsId INTEGER PRIMARY KEY, EmpSpn INTEGER, SlsTl REAL);
INSERT INTO COEMP(EmpId, Name)
VALUES(1,'Keith');
INSERT INTO COEMP(EmpId, Name)
VALUES(2,'Tommy');
INSERT INTO COEMP(EmpId, Name)
VALUES(3, 'Greg');
INSERT INTO PTSLS(SlsId, EmpSpn, SlsTl)
VALUES(000, 1, 10.00);
INSERT INTO PTSLS(SlsId, EmpSpn, SlsTl)
VALUES(001, 2, 20.00);
INSERT INTO PTSLS(SlsId, EmpSpn, SlsTl)
VALUES(002, 2, 10.00);
INSERT INTO PTSLS(SlsId, EmpSpn, SlsTl)
VALUES(003, 1, 20.00);
SELECT * FROM COEMP
OUTER JOIN PTSLS ON
COEMP.EmpId = PTSLS.EmpSpn;
